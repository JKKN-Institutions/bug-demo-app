# Demo App Environment Variables
# Copy this file to .env.local and update with your actual values

# Bug Reporter SDK Configuration
VITE_API_KEY=your-api-key-from-platform

# IMPORTANT: apiUrl should be the BASE URL only (without /api/v1/public)
# The SDK will automatically append /api/v1/public/bug-reports

# Local Development
# VITE_API_URL=http://localhost:3000

# Production (Vercel)
VITE_API_URL=https://jkkn-centralized-bug-reporter.vercel.app

# User Context (for demo purposes)
VITE_USER_ID=demo-user-123
VITE_USER_NAME=Demo User
VITE_USER_EMAIL=demo@example.com

# Debug Mode
VITE_DEBUG=true
